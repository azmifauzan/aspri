# JWT Configuration
SECRET_KEY=your-secret-key-here-change-this-in-production

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your-google-client-id-here

# Database Configuration
DATABASE_URL=mysql+aiomysql://user:password@localhost/aspri_db

# Other configurations
DEBUG=True

# LLM Embedding Configuration
# Choose one of: openai, anthropic, gemini
LLM_PROVIDER=openai
OPENAI_API_KEY=your-openai-api-key-here
OPENAI_EMBEDDING_MODEL=text-embedding-3-small
# For Anthropic/Gemini, you would add similar configuration
# ANTHROPIC_API_KEY=your-anthropic-api-key-here
# ANTHROPIC_EMBEDDING_MODEL=your-anthropic-model-here
# GEMINI_API_KEY=your-gemini-api-key-here
# GEMINI_EMBEDDING_MODEL=your-gemini-model-here